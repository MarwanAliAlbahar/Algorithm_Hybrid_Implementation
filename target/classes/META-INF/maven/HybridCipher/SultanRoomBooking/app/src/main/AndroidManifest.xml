<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sultan.bookingrooms"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sultan.bookingrooms.MainActivity"
            android:icon="@drawable/logo"
            android:label="Sultan's Rooms Booking" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sultan.bookingrooms.BookingAppServicesActivity"
            android:icon="@drawable/logo"
            android:label="Sultan's Rooms Booking" >
        </activity>
        <activity
            android:name="com.sultan.bookingrooms.AboutCompanyActivity"
            android:icon="@drawable/logo"
            android:label="Sultan's Rooms Booking" >
        </activity>
        <activity
            android:name="com.sultan.bookingrooms.ReachUsActivity"
            android:icon="@drawable/logo"
            android:label="Sultan's Rooms Booking" >
        </activity>
        <activity
            android:name="com.sultan.bookingrooms.UserRegisterOrSignInActivity"
            android:icon="@drawable/logo"
            android:label="Sultan's Rooms Booking" >
        </activity>
        <activity
            android:name="com.sultan.bookingrooms.UserSignInActivity"
            android:icon="@drawable/logo"
            android:label="Sultan's Rooms Booking" >
        </activity>
        <activity
            android:name="com.sultan.bookingrooms.UserSignUpActivity"
            android:icon="@drawable/logo"
            android:label="Sultan's Rooms Booking" >
        </activity>
        <activity
            android:name="com.sultan.bookingrooms.UserAccountBookingActivity"
            android:icon="@drawable/logo"
            android:label="Sultan's Rooms Booking" >
        </activity>
    </application>

</manifest>