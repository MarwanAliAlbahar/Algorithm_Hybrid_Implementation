<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv_label"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="Book Now"
        android:gravity="center_horizontal"
        android:textColor="#000000"
        android:textSize="16sp"
        android:background="@color/blue_color"
        android:textStyle="bold" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black" />
    <!-- package1 -->

    <LinearLayout
        android:id="@+id/package_item1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/img_place1"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:padding="2dp"
            android:src="@drawable/image1" />

        <RelativeLayout
            android:id="@+id/header_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingBottom="2dp"
            android:paddingTop="2dp" >

            <TextView
                android:id="@+id/tv_pricevalue1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:clickable="true"
                android:focusable="true"
                android:padding="5dp"
                android:text="@string/price1"
                android:textColor="#000000"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tv_place1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_pricevalue1"
                android:layout_marginTop="6dp"
                android:clickable="true"
                android:focusable="true"
                android:padding="5dp"
                android:text="@string/location1"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_services1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_place1"
                android:clickable="true"
                android:ellipsize="end"
                android:focusable="true"
                android:maxLines="3"
                android:padding="5dp"
                android:text="@string/desc1"
                android:textColor="#000000"
                android:textSize="12sp" />

            <Button
                android:id="@+id/btn_book_beoforelogin1"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="14dp"
                android:layout_marginTop="4dp"
                android:background="@color/blue_color"
                android:gravity="center"
                android:text="Book" />
        </RelativeLayout>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black" />
